<!doctype html>
<html>
<head>
    <meta name="charset" content="utf-8">
    <meta name="viewport" content="width=device-width">
    <title>{{title}}</title>
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.5.10/css/bootstrap-material-design.min.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.5.10/css/ripples.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/app.css">
</head>
<body>
<div id="root" class="root">
</div>
<script src="https://www.gstatic.com/firebasejs/3.2.0/firebase.js"></script>
<script>
    // Initialize Firebase
    var config = {
        apiKey: "{{fireBaseApiKey}}",
        authDomain: "{{fireBaseAuthDomain}}",
        databaseURL: "{{fireBaseDatabaseUrl}}",
        storageBucket: ""
    };

    firebase.initializeApp(config);
</script>
<script type="application/json" id="user">{{{user}}}</script>
<script type="text/javascript" src="//apis.daum.net/maps/maps3.js?apikey={{daumMapApiKey}}&libraries=services"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0/jquery.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.5.10/js/material.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.5.10/js/ripples.min.js"></script>
<script src="/js/dist/bundle.js"></script>
</body>
</html>
